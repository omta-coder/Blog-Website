<%- include("./partials/header")%>
<%- include("./partials/nav")%>
<div class="w-[90%] rounded  mx-auto bg-slate-500 mt-10 text-white p-10">
    <h1 class="text-2xl mb-5">Blog Website</h1>
    <div class="flex gap-3 flex-wrap">
        <% allBlogs.forEach((elem)=>{%>
            <div class="w-[10rem] h-[15vmax]  rounded bg-slate-700">
                <img class="w-full h-4/5 rounded " src="<%= elem.url%>" alt="">
                <p class="text-lg text-center"><%= elem.title%></p>
            </div>
            
            <%})%>
        </div>
</div>
<%- include("./partials/footer")%>
